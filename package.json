{
  "name": "wedata-test",
  "publisher": "frankrun",
  "displayName": "wedata test",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "test": [
        {
          "id": "testView",
          "name": "Test View",
          "newFile": true,
          "icon": "media/logo.svg"
        },
        {
          "id": "filesView",
          "name": "Files View",
          "icon": "media/logo.svg"
        },
        {
          "type": "webview",
          "id": "dbView",
          "name": "Database View",
          "icon": "media/logo.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "wedata-test.showCommandDialog",
        "title": "wedata: show Command Dialog"
      },
      {
        "command": "wedata-test.openSetting",
        "title": "open config",
        "icon": "$(gear)"
      },
      {
        "command": "wedata-test.openNewFile",
        "title": "Show Tree View",
        "icon": "$(new-file)"
      },
      {
        "command": "wedata-test.showHello",
        "title": "show Hello",
        "icon": "$(run)"
      },
      {
        "command": "wedata-test.refresh",
        "title": "showHello",
        "icon": "$(refresh)"
      },
      {
        "command": "wedata-test.showTreeView",
        "title": "Show Tree View",
        "icon": {
          "light": "media/add.svg",
          "dark": "media/add.svg"
        }
      },
      {
        "command": "wedata-test.hideTreeView",
        "title": "Hide Tree View"
      },
      {
        "command": "wedata-test.openWithExtension",
        "title": "Open with 我的插件"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "wedata-test.openSetting",
          "when": "view == testView"
        },
        {
          "command": "wedata-test.openNewFile",
          "when": "view == testView",
          "group": "navigation"
        },
        {
          "command": "wedata-test.refresh",
          "when": "view == testView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "wedata-test.showHello",
          "when": "view == testView",
          "group": "inline"
        },
        {
          "command": "wedata-test.showHello",
          "when": "view == testView"
        },
        {
          "command": "wedata-test.hideTreeView",
          "when": "view == testView"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.6.1",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
	"dependencies": {
		"@bendera/vscode-webview-elements": "^0.1.11-alpha.7",
		"vscode-codicons": "0.0.13"
	}
}
