{
	"name": "wedata-test",
	"description": "wedata test",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"test": [
				{
					"type": "webview",
					"id": "wedatatest.testView",
					"name": "wedata test"
				}
			]
		},
		"commands": [
			{
				"command": "extension.runCode",
				"title": "Run Code"
			  },
			{
				"command": "wedatatest.addTestFile",
				"category": "wedata test",
				"title": "add test",
				"icon": "$(new-file)"
			},
			{
				"command": "wedatatest.addTest",
				"category": "wedata test",
				"title": "add test item"
			},
			{
				"command": "wedatatest.cleartest",
				"category": "wedata test",
				"title": "Clear test",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "wedatatest.cleartest",
					"group": "navigation",
					"when": "view == wedatatest.testView"
				},
				{
					"command": "wedatatest.addTestFile",
					"group": "navigation",
					"when": "view == wedatatest.testView"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"fs": "^0.0.1-security"
	}
}
